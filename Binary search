arr = [1,2,3,4,5,6]
target = 4
low = arr[0]
high = arr[-1]
def binarySearch(arr, target, low, high):
	while low <= high:

		mid = low + (high - low) // 2

		if arr[mid] == target:
			return mid

		elif arr[mid] < target:
			low = mid + 1

		else:
			high = mid - 1

	return -1

print(binarySearch(arr, target, low, high))
